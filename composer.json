{
    "name": "nelsonrrj/test",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nelson Rodriguez"
        }
    ],
    "require": {
        "doctrine/dbal": "^4",
        "doctrine/orm": "^3",
        "php-di/php-di": "^7.0",
        "symfony/cache": "^7",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "scripts": {
        "test": "phpunit",
        "test:unit": "phpunit tests/Unit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable"
}
